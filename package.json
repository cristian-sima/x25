{
  "version": "1.0.0",
  "scripts": {
    "dist": "node conf/dist.js & webpack --display-error-details --mode production --optimize-minimize --progress -p",
    "eslint": "eslint . --ext .jsx,.js --ignore-pattern client/styles --fix",
    "flow": "flow check --color=always",
    "start": "node conf/dev.js & node server.js",
    "test": "jest",
    "check": "npm-check-updates -a && npm update",
    "test:watch": "jest --watch"
  },
  "babel": {
    "presets": [
      "env",
      "stage-0",
      "react"
    ],
    "env": {
      "production": {
        "plugins": [
          "transform-decorators-legacy",
          "react-html-attrs",
          "transform-class-properties",
          "transform-react-constant-elements",
          "transform-react-inline-elements",
          "transform-react-remove-prop-types",
          "transform-flow-strip-types"
        ]
      },
      "developmentTime": {
        "plugins": [
          "transform-decorators-legacy",
          "react-hot-loader/babel",
          "flow-react-proptypes",
          "react-html-attrs",
          "transform-class-properties"
        ]
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/cristian-sima/house.git"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">= 6.3.1"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/conf/polyfills/tempPolyfills.js"
    ],
    "testRegex": "client/.*-test.js$",
    "modulePaths": [
      "client"
    ]
  },
  "devDependencies": {
    "webpack-dev-server": "^3.1.9",
    "npm-check-updates": "^2.14.2"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-flow-react-proptypes": "^24.1.2",
    "babel-plugin-react-html-attrs": "^2.1.0",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.18",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bootstrap": "^4.1.3",
    "css-loader": "^1.0.0",
    "dezalgo": "^1.0.3",
    "eslint": "^5.6.1",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "flow-bin": "^0.81.0",
    "flow-status-webpack-plugin": "^0.1.7",
    "font-awesome": "^4.7.0",
    "history": "^4.7.2",
    "humps": "^2.0.1",
    "immutable": "^4.0.0-rc.10",
    "jest": "^23.6.0",
    "jest-immutable-matchers": "^2.0.1",
    "json-loader": "^0.5.7",
    "lodash.memoize": "^4.1.2",
    "moment": "^2.22.2",
    "node-sass": "^4.9.3",
    "normalizr": "^3.2.4",
    "postcss-loader": "^3.0.0",
    "query-string": "^6.1.0",
    "raphael": "^2.2.7",
    "react": "^16.5.2",
    "react-addons-shallow-compare": "^15.6.2",
    "react-autosuggest": "^9.4.2",
    "react-dom": "^16.5.2",
    "react-dropzone": "^5.1.1",
    "react-hot-loader": "^4.3.11",
    "react-notification-system-redux": "^1.2.0",
    "react-raphael": "^0.9.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-sidebar": "^3.0.2",
    "reactstrap": "^6.4.0",
    "redux": "^4.0.0",
    "redux-form": "^7.4.2",
    "redux-immutable": "^4.0.0",
    "redux-paginator-immutable": "^1.6.3",
    "redux-promise-middleware": "^5.1.1",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.3.0",
    "redux-ui": "0.1.1",
    "reselect": "^4.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "superagent": "^4.0.0-beta.5",
    "superagent-promise": "^1.1.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-git-hash": "^1.0.2",
    "webpack-preset": "^0.2.0",
    "webpack-preset-babel": "^0.2.0",
    "webpack-preset-babel-stage-2": "^0.2.0"
  },
  "author": "Cristian Sima"
}
